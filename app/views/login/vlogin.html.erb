<%= form_tag('/login', method: "post") do%>

<%= label_tag(:address, "Email") %>
<%= email_field(:user, :address) %><br>

<%= label_tag(:password, "password ") %>
<%= password_field_tag(:password)%><br>

<%= submit_tag 'Login',:class=>("btn waves-effect waves-light col s12")%>
<%end%>
<br>
<br>
<%= link_to "Login", url_for(controller: :login, action: :vregistration),:class=>("btn waves-effect waves-light col s12") , method: :get%>
