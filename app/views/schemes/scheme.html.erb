  <div class="col s12">
      <ul class="collapsible popout" data-collapsible="accordion">
        <li class="collection-header"><h4>Schemes</h4></li>



                   <% @res.each do |t|%>
                   <li>
               <div class="collapsible-header"><%= t["name"]%><i class="material-icons right">add</i></div>
               <div class="collapsible-body"><div class="row"></div>

                    <%= t["details"]%>
                    <% slug=t["slug"]%>
                    <%= link_to "For More Click Here", url_for(controller: :schemes, action: :register_scheme,:scheme_name=>slug), method: :post%>
                  </li>
                    <%end%>



       </ul>
  </div>
</div>
