<%= form_tag('/aregistration', method: "post") do%>

<%= label_tag(:name, "Name") %>
<%= text_field(:name) %><br>


<%= label_tag(:address, "Email") %>
<%= email_field(:user, :address) %><br>

<%= label_tag(:password, "password ") %>
<%= password_field_tag(:password)%><br>

<%= submit_tag 'Register ',:class=>("btn waves-effect waves-light col s12")%>
<%end%>

