
<br>
<br>

<%= form_tag('/Addscheme', method: "post") do%>


<%= label_tag(:name, "Name Of Scheme") %>
<%= text_field_tag(:name) %><br>

<%= label_tag(:address, "Url") %>
<%= email_field(:user, :address) %><br>

<%= label_tag(:start_date, "Start Date:") %>
<%= date_field(:user, :start_date,:value=>"1997-09-04") %>

<%= label_tag(:end_date, "End Date:") %>
<%= date_field(:user, :end_date,:value=>"1997-12-04") %>

<%= label_tag(:draw_date, "Draw Date:") %>
<%= date_field(:user, :draw_date,:value=>"1997-12-30") %>

<%= submit_tag 'Add Scheme',:class=>("btn waves-effect waves-light col s12")%>
<%end%>

